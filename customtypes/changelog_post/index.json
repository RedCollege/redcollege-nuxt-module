{
  "format": "page",
  "id": "changelog_post",
  "label": "Changelog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "fecha_del_changelog": {
        "type": "Timestamp",
        "config": {
          "label": "Fecha Del Changelog",
          "placeholder": ""
        }
      },
      "titulo_del_changelog": {
        "type": "Text",
        "config": {
          "label": "Título del Changelog",
          "placeholder": ""
        }
      },
      "modulo": {
        "type": "Select",
        "config": {
          "label": "Modulo",
          "placeholder": "",
          "options": ["Dashboard", "Planificaciones", "Enfermería"],
          "default_value": "Dashboard"
        }
      },
      "resumen_de_cambios": {
        "type": "StructuredText",
        "config": {
          "label": "Resumen de Cambios",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
